{
  "name": "@hospitalrun/cli",
  "version": "1.0.0",
  "description": "Command line interface for HospitalRun applications",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "bin": {
    "hospitalrun": "./dist/hospitalrun.js"
  },
  "scripts": {
    "commit": "npx git-cz",
    "test": "echo \"Error: no test specified\" && exit 0",
    "prepareOnly": "npm run build",
    "prebuild": "npm run test",
    "build": "tsdx build",
    "semantic-release": "semantic-release"
  },
  "homepage": "https://github.com/HospitalRun/hospitalrun-cli#readme",
  "bugs": {
    "url": "https://github.com/HospitalRun/hospitalrun-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HospitalRun/hospitalrun-cli.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "~8.2.0",
    "@commitlint/config-conventional": "~8.2.0",
    "@commitlint/core": "~8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.3",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@types/fs-extra": "~8.0.0",
    "@types/node": "~12.7.4",
    "commitizen": "~4.0.3",
    "commitlint-config-cz": "^0.12.1",
    "cross-env": "~6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "dateformat": "~3.0.3",
    "husky": "~3.0.7",
    "semantic-release": "^15.13.24",
    "tsdx": "~0.11.0",
    "tslib": "~1.10.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "colors": "~1.4.0",
    "commander": "~4.0.1",
    "cross-spawn": "~7.0.0",
    "inquirer": "~7.0.0"
  }
}
